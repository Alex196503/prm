@page
@model ProiectMediiBun.Pages.Reservations.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Reservation[0].Data_Rezervarii)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reservation[0].Durata)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reservation[0].Member)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reservation[0].Teren)
            </th>
            <th></th>
        </tr>
